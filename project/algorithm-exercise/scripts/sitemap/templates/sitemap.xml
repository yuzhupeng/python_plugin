<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for page in pages %}
    <url>
        <loc>{{root_url}}/{{page['url']}}</loc>
        <lastmod>{{page['lastmod']}}</lastmod>
        <changefreq>{{freq}}</changefreq>
    </url>
    {% endfor %}
</urlset>
